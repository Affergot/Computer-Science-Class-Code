import requests
import json

try:
    with open('acid-arrow.json', 'r') as file:
        acid-arrow = json.load(file)
except FileNotFoundError:
    database = {}
    database["Creation"] = "True"
    with open('acid-arrow.json', 'w') as fp:
        json.dump(database, fp)

url = "https://www.dnd5eapi.co/api/spells/acid-arrow/"

response = requests.get(url)

with open("acid-arrow.json", "w") as fp
    json.dump(response, fp)

print(response)
